{
  "name": "automart",
  "version": "1.0.0",
  "description": "Auto Mart is an online marketplace for automobiles of diverse makes, model or body type. With Auto Mart, users can sell their cars or buy from trusted dealerships or private sellers",
  "main": "index.js",
  "scripts": {
    "start": "babel-node dist/index.js",
    "build": "babel src --out-dir dist",
    "dev": "nodemon --exec babel-node src/index.js",
    "test": "nyc --reporter=html --reporter=text mocha --require @babel/register 'src/**/**test.js' --exit",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/didasmbalanya/AutoMart/issues"
  },
  "homepage": "https://github.com/didasmbalanya/AutoMart#readme",
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.11",
    "validator": "^11.0.0",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
